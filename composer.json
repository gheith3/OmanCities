{
    "name": "gheith3/omancities",
    "description": "for migrate oman governorates and cities",
    "keywords": [
        "gheith3",
        "laravel",
        "omancities"
    ],
    "homepage": "https://github.com/gheith3/omancities",
    "license": "MIT",
    "authors": [
        {
            "name": "Gheith Alrawahi",
            "email": "alrawahi.gheith@gmil.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.9.2",
        "spatie/laravel-translatable": "^5.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.10",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.4",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Gheith3\\OmanCities\\": "src",
            "Gheith3\\OmanCities\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gheith3\\OmanCities\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gheith3\\OmanCities\\OmanCitiesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
